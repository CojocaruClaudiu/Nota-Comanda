{
    "name":  "topaz-server",
    "scripts":  {
                    "prisma:gen":  "prisma generate",
                    "migrate:order-status":  "prisma db execute --file ./sql/extend_order_status.sql --schema ./prisma/schema.prisma",
                    "backfill:material-pack-info":  "tsx scripts/material-pack-conversion.ts",
                    "start":  "node dist/index.js",
                    "backfill:item-units":  "tsx scripts/backfill-operation-item-units.ts",
                    "prisma:seed":  "tsx prisma/seed.ts",
                    "seed:equipment":  "tsx scripts/import-equipment.ts",
                    "seed:employees":  "tsx scripts/import-employees.ts",
                    "build":  "tsc",
                    "backup":  "powershell -ExecutionPolicy Bypass -File scripts/backup-database.ps1",
                    "seed:producers":  "tsx scripts/import-producers.ts",
                    "import:cnp":  "tsx scripts/import-cnp.ts",
                    "delete:materials":  "tsx scripts/delete-all-materials.ts",
                    "seed:equipment-full":  "tsx scripts/import-equipment-full.ts",
                    "reseed:materials":  "npm run delete:materials \u0026\u0026 npm run seed:materials",
                    "seed:materials":  "tsx scripts/import-materials.ts",
                    "reseed:operation-items":  "npm run wipe:operation-items \u0026\u0026 npm run seed:operation-items \u0026\u0026 npm run backfill:item-units",
                    "dev":  "tsx watch src/index.ts",
                    "seed:operation-items":  "tsx scripts/add-operation-items.ts",
                    "test":  "jest --runInBand",
                    "prisma:migrate":  "prisma migrate dev",
                    "backfill:equipment-categories":  "tsx scripts/backfill-equipment-categories.ts",
                    "wipe:operation-items":  "tsx scripts/wipe-operation-items.ts",
                    "seed:suppliers":  "tsx scripts/import-suppliers-from-json.ts"
                },
    "devDependencies":  {
                            "jest":  "^29.7.0",
                            "prisma":  "6.16.2",
                            "tsx":  "^4.19.2",
                            "@types/cors":  "^2.8.19",
                            "@types/jsonwebtoken":  "^9.0.10",
                            "@types/node":  "^24.7.1",
                            "@types/jest":  "^29.5.12",
                            "@types/file-saver":  "^2.0.7",
                            "typescript":  "^5.5.4",
                            "@types/bcrypt":  "^6.0.0",
                            "ts-jest":  "^29.2.5",
                            "@types/express":  "^5.0.3"
                        },
    "type":  "module",
    "dependencies":  {
                         "helmet":  "^7.1.0",
                         "dotenv":  "^16.4.5",
                         "bcrypt":  "^5.1.1",
                         "express":  "^4.19.2",
                         "jsonwebtoken":  "^9.0.2",
                         "axios":  "^1.12.2",
                         "@types/multer":  "^2.0.0",
                         "@prisma/client":  "6.16.2",
                         "cors":  "^2.8.5",
                         "zod":  "^3.23.8",
                         "multer":  "^2.0.2",
                         "xlsx":  "^0.18.5",
                         "@tanstack/react-query":  "^5.87.1"
                     },
    "private":  true
}
